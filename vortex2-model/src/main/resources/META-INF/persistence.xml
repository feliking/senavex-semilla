<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.1" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
    <persistence-unit name="vortex2-store" transaction-type="JTA">
        <jta-data-source>java:comp/env/jdbc/vortex2-store</jta-data-source>
        <class>bo.gob.senavex.vortex2.model.CliCategoria</class>
        <class>bo.gob.senavex.vortex2.model.CliContacto</class>
        <class>bo.gob.senavex.vortex2.model.CliDireccion</class>
        <class>bo.gob.senavex.vortex2.model.CliDocumento</class>
        <class>bo.gob.senavex.vortex2.model.CliEmpresa</class>
        <class>bo.gob.senavex.vortex2.model.CtaPago</class>
        <class>bo.gob.senavex.vortex2.model.CtaDeposito</class>
        <class>bo.gob.senavex.vortex2.model.LogCambio</class>
        <class>bo.gob.senavex.vortex2.model.LogConsulta</class>
        <class>bo.gob.senavex.vortex2.model.LogEvento</class>
        <class>bo.gob.senavex.vortex2.model.LogSesion</class>
        <class>bo.gob.senavex.vortex2.model.ParDominio</class>
        <class>bo.gob.senavex.vortex2.model.ParValor</class>
        <class>bo.gob.senavex.vortex2.model.RegRegistro</class>
        <class>bo.gob.senavex.vortex2.model.RegSeguimiento</class>
        <class>bo.gob.senavex.vortex2.model.SegCorreo</class>
        <class>bo.gob.senavex.vortex2.model.SegOperador</class>
        <class>bo.gob.senavex.vortex2.model.SegPersona</class>
        <class>bo.gob.senavex.vortex2.model.SegUsuario</class>
        <validation-mode>NONE</validation-mode>
        <properties>
            <property name="javax.persistence.schema-generation.database.action" value="none"/>
            <property name="hibernate.cache.use_query_cache" value="true"/>
            <property name="hibernate.dialect" value="org.hibernate.dialect.PostgreSQLDialect"/>
        </properties>
    </persistence-unit>
</persistence>
